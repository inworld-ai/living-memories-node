{
  "name": "inworld-lipsync-memory-demo",
  "version": "1.0.0",
  "main": "server.ts",
  "scripts": {
    "start": "npx ts-node server.ts",
    "dev": "npx ts-node server.ts",
    "build": "npx tsc",
    "clean": "node -e \"const fs=require('fs');const path=require('path');const dir=path.join(__dirname,'uploads');if(fs.existsSync(dir)){fs.readdirSync(dir).forEach(f=>fs.unlinkSync(path.join(dir,f)));}\"",
    "clean:win": "cd uploads && del /Q *.*",
    "clean:unix": "rm -rf uploads/*"
  },
  "author": "Inworld AI",
  "license": "MIT",
  "description": "Inworld AI Demo: LipSync Video Generation & Voice-Based Memory Companion with TTS, STT, and LLM integration",
  "dependencies": {
    "@inworld/runtime": "0.6.0",
    "dotenv": "^17.2.1",
    "uuid": "^11.1.0",
    "express": "^4.18.2",
    "multer": "^1.4.5-lts.1",
    "cors": "^2.8.5",
    "form-data": "^4.0.0",
    "node-fetch": "^2.6.7"
  },
  "devDependencies": {
    "@tsconfig/node-lts": "^22.0.2",
    "@types/node": "^24.1.0",
    "@types/express": "^4.17.17",
    "@types/multer": "^1.4.7",
    "@types/cors": "^2.8.13",
    "@types/node-fetch": "^2.6.4",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3"
  }
}
